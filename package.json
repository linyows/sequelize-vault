{
  "name": "sequelize-vault",
  "version": "3.0.0",
  "description": "A Sequelize plugin for easily integrating Hashicorp Vault",
  "main": "index.js",
  "types": "lib/sequelize-vault.d.ts",
  "files": [
    "LICENSE",
    "README.md",
    "index.js",
    "lib"
  ],
  "directories": {
    "lib": "lib",
    "src": "src"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "sequelize": "^4.37.7"
  },
  "devDependencies": {
    "@types/node": "11.11.6",
    "@types/sequelize": "^4.27.17",
    "@types/sqlite3": "^3.1.3",
    "ava": "^1.0.1",
    "ava-ts": "^0.25.0",
    "coveralls": "^3.0.1",
    "nyc": "^13.2.0",
    "pg": "^7.4.3",
    "reflect-metadata": "^0.1.12",
    "sequelize-typescript": "^0.6.4",
    "sqlite3": "^4.0.0",
    "testdouble": "^3.8.1",
    "ts-node": "^8.0.3",
    "tslint": "^5.10.0",
    "tslint-microsoft-contrib": "^6.0.0",
    "typescript": "^2.8.3"
  },
  "engines": {
    "node": ">=8.4.0"
  },
  "ava": {
    "files": [
      "src/**/*.test.ts"
    ]
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "src/**/*.test.ts"
    ]
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf lib",
    "ci": "npm install && npm run lint && npm run db:create && nyc ava-ts -v && npm audit",
    "test": "npm run db:create && ava-ts -v ; npm run db:drop",
    "cov": "nyc npm test",
    "lint": "tslint -p tsconfig.json -c tslint.json src/**/*.ts",
    "preversion": "npm run clean && npm run build && npm test",
    "postversion": "git push origin --tags",
    "prepublishOnly": "npm run clean && npm run build && npm test",
    "db:create": "createuser sequelizevault && createdb sequelizevault",
    "db:drop": "dropdb sequelizevault && dropuser sequelizevault"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/linyows/sequelize-vault.git"
  },
  "keywords": [
    "sequelize",
    "vault"
  ],
  "author": "linyows",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/linyows/sequelize-vault/issues"
  },
  "homepage": "https://github.com/linyows/sequelize-vault"
}
